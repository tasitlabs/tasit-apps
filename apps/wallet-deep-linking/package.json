{
  "main": "node_modules/expo/AppEntry.js",
  "version": "0.0.3",
  "scripts": {
    "start": "expo start -c",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "build:android": "expo build:android",
    "build:ios": "expo build:ios",
    "lint": "npx prettier --write '{*.js,**/*.js,*.jsx,**/*.jsx,*.ts,**/*.ts,*.tsx,**/*.tsx}' && npx eslint '{*.js,**/*.js,*.jsx,**/*.jsx,*.ts,**/*.ts,*.tsx,**/*.tsx,**/*.snap}' --fix",
    "eject": "expo eject",
    "pretest": "npm run lint",
    "test:update": "npx jest -u --maxWorkers=2",
    "test": "npx jest --maxWorkers=2",
    "update": "npx ncu -x 'react, react-dom, babel-preset-expo, @types/react, @types/react-native, @types/jest, react-navigation, jest, jest-expo, eslint-plugin-jest, eslint-plugin-react, react-native, react-native-gesture-handler, react-native-reanimated, react-native-appearance, react-navigation-drawer, react-navigation-stack, react-navigation-tabs'",
    "update:write": "npm run update -- -u"
  },
  "jest": {
    "preset": "jest-expo",
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)?$": "ts-jest"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tasitlabs/tasit.git"
  },
  "dependencies": {
    "expo": "^38.0.8",
    "react": "16.11.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-38.0.0.tar.gz",
    "react-native-appearance": "~0.3.3",
    "react-native-gesture-handler": "~1.6.0",
    "react-native-reanimated": "~2.10.0",
    "react-navigation": "^4.0.10",
    "react-navigation-drawer": "^2.3.3",
    "react-navigation-stack": "^1.10.3",
    "react-navigation-tabs": "^2.5.6",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/react": "~16.9.41",
    "@types/react-native": "~0.62.13",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "babel-eslint": "^10.1.0",
    "babel-preset-expo": "^8.2.3",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.0.4",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-native": "^3.8.1",
    "jest": "^24.9.0",
    "jest-expo": "^38.0.0",
    "npm-check-updates": "^7.0.1",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.1",
    "ts-node": "^8.10.2",
    "typescript": "~3.9.6"
  },
  "private": true,
  "license": "MIT",
  "author": "Tasit Labs",
  "bugs": {
    "url": "https://github.com/tasitlabs/tasit/issues"
  },
  "homepage": "https://tasit.io"
}
